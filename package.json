{
  "name": "brainly",
  "version": "0.0.1",
  "description": "Service for learning programming",
  "main": "main.go",
  "license": "MIT",
  "scripts": {
    "run": "better-npm-run run",
    "vendor:save": "better-npm-run vendor:save",
    "build:server": "better-npm-run build:server",
    "webpack": "better-npm-run webpack",
    "webpack-dev-server": "webpack-dev-server --config ./config/webpack.config.js --progress --colors",
    "start:server": "better-npm-run start:server",
    "generate:docs": "better-npm-run generate:docs",
    "vendor:restore": "better-npm-run vendor:restore"
  },
  "betterScripts": {
    "start": {
      "command": "echo 'No run command yet!' && exit 1;"
    },
    "run": {
      "command": "go run ./main.go",
      "env": {
        "PORT": 5000,
        "DATABASE_URL": ""
      }
    },
    "webpack": "webpack --config ./config/webpack.config.js",
    "vendor:save": "godep save && yarn",
    "vendor:restore": "godep restore",
    "build:server": "go build ./main.go",
    "start:server": {
      "command": "./main",
      "env": {
        "PORT": 5000,
        "DATABASE_URL": ""
      }
    },
    "generate:docs": "swagger generate spec -o ./docs/swagger.json"
  },
  "dependencies": {
    "auth0-lock": "^10.11.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-notifier": "^1.5.0"
  }
}
